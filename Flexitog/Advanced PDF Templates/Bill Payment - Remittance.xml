<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
<#if .locale == "ru_RU">
    <link name="verdana" type="font" subtype="opentype" src="${nsfont.verdana}" src-bold="${nsfont.verdana_bold}" bytes="2" />
</#if>
    <macrolist>
        <macro id="nlheader">
            <!-- Header -->
<table style="font-size: 7.5pt; color:#888888; padding-left:0px; padding-right:0px;"><tr>
	<td align="center" colspan="1">&nbsp;</td>
	<!-- Contact details -->
	<td align="left" colspan="3" valign="middle">
	<table border="0" cellpadding="0" style="font-size: 7.5pt; color:#888888;"><tr style="padding: 1px 0px;">
		<td align="left" colspan="1"><img height="10" src="https://system.netsuite.com/c.1294017/template-images/icon-phone.jpg" width="10" /></td>
		<td algin="left" colspan="11">&nbsp;+44 (0) 1692 400 300</td>
		</tr>
		<tr style="padding: 1px 0px;">
		<td align="left" colspan="1"><img height="10" src="https://system.netsuite.com/c.1294017/template-images/icon-email.jpg" width="10" /></td>
		<td algin="left" colspan="11" style="padding-left:5px;">&nbsp;customerservice@flexitog.com</td>
		</tr>
		<tr style="padding: 1px 0px;">
		<td align="left" colspan="1"><img height="10" src="https://system.netsuite.com/c.1294017/template-images/icon-web.jpg" width="10" /></td>
		<td algin="left" colspan="11" style="padding-left:5px;">&nbsp;www.flexitog.com</td>
		</tr></table>
	</td>
	<!-- Logo -->
	<td align="center" colspan="4" id="logo"><img src="https://system.netsuite.com/c.1294017/template-images/FlexiTog-Form-Logo-2017.png" /></td>
	<!-- Address -->
	<td align="right" colspan="3" style="line-height: 11pt;" valign="middle">
	<p align="right">The Granary Business Park<br />School Road, Neatishead<br />Norfolk, NR12 8BU</p>
	</td>
	<td align="center" colspan="1">&nbsp;</td>
	</tr></table>
        </macro>
        <macro id="nlfooter">
            <table style="width: 100%; font-size: 7.5pt; color:#888888; margin-top:0px;"><tr>
	<td align="right" colspan="1" style="padding: 0;">&nbsp;</td>
	<td align="center" colspan="10" style="padding: 0;">FlexiTog<sup>&reg;</sup> is a registered brand and trading name of JGM International | VAT Registration no. GB225 3187 77</td>
	<td align="right" colspan="1" style="padding: 0;"><pagenumber/>/<totalpages/></td>
	</tr></table>
        </macro>
    </macrolist>
    <style type="text/css">body{margin-top:0px; padding-top:0px;}
      table {
        <#if .locale == "zh_CN">
            font-family: stsong, sans-serif;
        <#elseif .locale == "zh_TW">
            font-family: msung, sans-serif;
        <#elseif .locale == "ja_JP">
            font-family: heiseimin, sans-serif;
        <#elseif .locale == "ko_KR">
            font-family: hygothic, sans-serif;
        <#elseif .locale == "ru_RU">
            font-family: verdana;
        <#else>
            font-family: sans-serif;
        </#if>
            font-size: 8pt;
            table-layout: fixed;
        width:100%
        }


        th {
            font-weight: bold;
            font-size: 8pt;
            vertical-align: top;
            padding: 10px 0px 10px 0px;
            background-color: #333333;
            color: #ffffff;
        }
              
        td {
            padding: 4px 6px;
            color:#868686;
        }

        td#logo{background-color:#cf0a2c; padding: 32px 0px 28px;}
        td#logo img{width:40%; height:40%;}
        /*item table style*/
        table.itemtable > thead > tr > th {
      background-color: #ffffff;
            color: #333333;
            font-style: italic;
            font-size: 10pt;
            vertical-align: bottom;
            font-weight:400;
            padding: 20px 4px 5px 4px;
            border-top:0px;
          }
      table.itemtable > tr {
      padding:0px;
            margin:0px;
          border-bottom:2px solid #ffffff
        }
    table.itemtable > td {
         padding:4px;
         vertical-align:top;
        }
      table.itemtable > tbody > tr > td:first-child, table.itemtable > tbody > tr > td:last-child {background-color:#ffffff; border-bottom:2px solid #ffffff }
      
  table.totals td {
      padding-top:2px;
            padding-bottom:2px;
        }
      
      table.totals > tbody > tr >  td:first-child {
      padding-top:4px;
            padding-bottom:2px;
        }
       table.totals > tbody > tr >  td:last-child {
      padding-top:2px;
            padding-bottom:4px;
        }
      /*inner table*/
      table.inner tr {
          padding:0px;
            margin:0px;
          border-bottom:0px;
      }
      table.inner td {
         padding:0px 4px;
         vertical-align:top;}
      
      
      .underline {
        border-top:2px solid #ffffff!important;
                border-left:0px solid #ffffff!important;
                border-right:0px solid #ffffff!important;
                border-bottom: 2px solid #333333;
                padding-bottom:4px!important;
}
      

      
        /* font styling*/
      
      td.fs4, span.fs4{font-size:4pt}
          td.fs7, span.fs7{font-size:7pt}
      td.fs7-5{font-size:7.5pt}
          td.fs8{font-size:8pt}
      td.fs9{font-size:9pt}
      td.fs10, th.fs10{font-size:10pt}
          td.fs12, tr.fs12{font-size:12pt}
      td.fs16, tr.fs16{font-size:16pt}
          td.fs26{font-size:26pt}
          td.fs32{font-size:32pt}
      td.light{font-weight:300;}
      td.normal, th.normal, p.normal, span.normal, .normal{font-weight:400;}
      td.bold, p.bold, span.bold, .bold{font-weight:700;}
      td.heavy, p.heavy, span.heavy, .heavy{font-weight:800;}
      td.italic, th.italic, p.italic, span.italic, .italic{font-style:italic;}
      td.c-black, span.c-dark{color:#1a1a1a;}
          td.c-dark, th.c-dark, span.c-dark{color:#333333;}
      td.c-med, span.c-med{color:#888888;}
      td.c-white, tr.c-white{color:#ffffff;}
          td.c-red, tr.c-red{color:#cf0a2c;}
      td.ar{text-align:right!important;}
            td.tt-lc{text-transform:lowercase;}
            td.tt-uc{text-transform:uppercase;}
          td.tt-sc{text-transform:capitalize;}
            td.ws0{word-spacing:0px;}
      /* Background Colours */
          td.dark, tr.dark{background-color:#333333;}
          td.med, tr.med, th.med, .med{background-color:#ededed;}
          td.med-light, tr.med-light{background-color:#f1f1f1;}
          td.white, tr.white{background-color:#ffffff;}
      /* Padding and margins */
          td.pd-0, tr.pd-0{padding:0pxImportant;}
          td.pdtb-10{padding:10px 0px;}
          td.pdb-10{padding-bottom:10px;}
          td.pdr-2{padding-right:2px}
          .ma-t-10{margin-top:10px;}
          tr.pd-2{padding:2px}
</style>
</head>
<body header="nlheader" header-height="125" footer="nlfooter" footer-height="20" padding="0mm 0mm 0mm 0mm" size="A4">
 <!-- **************************************  DOCUMENT TITLE AND NUMBER **************************************  -->
<table cellpadding="0"><tr>
	<td align="center" colspan="1">&nbsp;</td>
	<td align="left" class="fs26 c-dark" colspan="10">${record@title} (Remittance)</td>
	<td align="right" class="fs16 c-dark ar " colspan="3">
	<p align="right"><span class="fs7"></span></p>
	</td>
	<td align="center" colspan="1">&nbsp;</td>
	</tr></table>
<!-- **************************************  DOC ADRESSES DATE AND AMOUNTS **************************************  -->

<table cellpadding="0" style="width: 100%; margin-top:10px;"><!-- Titles --><tr style="padding:5px">
	<td align="center" colspan="1">&nbsp;</td>
	<td align="left" class="fs10 c-grey tt-sc" colspan="7">${record.entity@label}: ${record.entity}</td>
	
	<td align="right" class="fs10 c-dark ar" colspan="3" style="padding-right:2px;">${record.trandate@label} <b>${record.trandate}</b></td>
	<td align="center" colspan="1">&nbsp;</td>
	</tr>
	<!-- Details -->
	<tr style="padding-bottom:10px; ">
	<td align="center" colspan="1">&nbsp;</td>
	<td align="left" colspan="7" style="padding-right:8px;">
	<p align="left">${record.address@label}<br/>${record.address}</p>
	</td>
	<td align="right" colspan="3">
	<table cellpadding="0" style="padding-bottom:10px; "><tr>
		<td align="right" class="fs10 c-dark bold ar">Amount Paid</td>
		</tr>
		<tr>
		<td align="right" class="fs26 c-dark bold ar">${record.total} <#if record.currency?contains("US Dollar")><span style="font-size:12px;">USD</span></#if></td>
		</tr></table>
	</td>
	<td align="center" colspan="1">&nbsp;</td>
	</tr></table>
<!-- ************************************** ACCOUNT DETAILS ************************************** -->

<table border="0" style="width: 100%;">
<thead>
	<tr>
	<th align="center" class="pb-0" colspan="1">&nbsp;</th>
	
	</tr>
</thead>
</table>
<#if record.apply?has_content>
<table class="itemtable" style="width: 100%; margin-top: 0px;"><!-- start apply sublist --><#list record.apply as apply><#if apply_index==0>
<thead>
	<tr>
    <th align="center" colspan="1">&nbsp;</th> 
	<th align="center" colspan="3">${apply.applydate@label}</th>
	<th colspan="4">${apply.type@label}: ${apply.refnum@label}</th>
	<th align="right" colspan="3">${apply.disc@label}</th>
	<th align="right" colspan="3">${apply.amount@label}</th>
    <th align="center" colspan="1">&nbsp;</th>
	</tr>
</thead>
</#if>
	<tr style="background-color: ${((apply_index % 2)==0)?string('#f6f6f6', '#ffffff')};  border-bottom: 2px solid #ffffff;">
    <td align="center" colspan="1">&nbsp;</td>  
	<td align="center" colspan="3" line-height="150%">${apply.applydate}</td>
	<td colspan="4">${apply.type}: ${apply.refnum} ${apply.description}</td>
	<td align="right" colspan="3">${apply.disc}</td>
	<td align="right" colspan="3">${apply.amount}</td>
    <td align="center" colspan="1">&nbsp;</td>
	</tr>
	</#list><!-- end apply -->
  	<tr class="">
    <td align="center" colspan="1">&nbsp;</td>  
	<td align="center" colspan="3" line-height="150%"></td>
	<td colspan="4"></td>
    <td height="24" align="right" colspan="6" class="dark c-white bold fs16" >Total Paid: ${record.total}</td>
    <td align="center" colspan="1">&nbsp;</td>
	</tr>
</table>
</#if>

</body>
</pdf>
