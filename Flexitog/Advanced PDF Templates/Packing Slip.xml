<?xml version="1.0"?>
<!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
<#if .locale == "ru_RU">
<link name="verdana" type="font" subtype="opentype" src="${nsfont.verdana}" src-bold="${nsfont.verdana_bold}" bytes="2" />
</#if>

     <macrolist>
        <macro id="nlheader">

      
          <table style="font-size: 7.5pt; color:#888888; padding-left:0px; padding-right:0px;">
              <tr>
                  <td align="center" colspan="1">&nbsp;</td>
               
                  <td align="left" valign="middle" colspan="3" >
                    <table border="0" cellpadding="0" style="font-size: 7.5pt; color:#888888;">
                      <tr style="padding: 1px 0px;"><td align="left" colspan="1"><img src="https://system.netsuite.com/c.1294017/template-images/icon-phone.jpg" width="10" height="10" /></td><td algin="left" colspan="11">&nbsp;+44 (0) 1692 400 300</td></tr>
                        <tr style="padding: 1px 0px;"><td align="left" colspan="1"><img src="https://system.netsuite.com/c.1294017/template-images/icon-email.jpg" width="10" height="10"  /></td><td algin="left" colspan="11" style="padding-left:5px;">&nbsp;customerservice@flexitog.com</td></tr>
                      <tr style="padding: 1px 0px;"><td align="left" colspan="1"><img src="https://system.netsuite.com/c.1294017/template-images/icon-web.jpg" width="10" height="10"  /></td><td algin="left" colspan="11" style="padding-left:5px;">&nbsp;www.flexitog.com</td></tr>
                    </table>
                        
                </td>
                  
                  <td align="center" id="logo" colspan="4"><img src="https://system.netsuite.com/c.1294017/template-images/FlexiTog-Form-Logo-2017.png"/></td>
                  
                  <td align="right" valign="middle" colspan="3" style="line-height: 11pt;"><p align="right">The Granary Business Park<br/> School Road, Neatishead<br/> Norfolk, NR12 8BU</p></td>
                  <td align="center" colspan="1">&nbsp;</td>
              </tr>  
          </table>

        </macro>
      
        <macro id="nlfooter">

            <table style="width: 100%; font-size: 7.5pt; color:#888888; margin-top:0px;">
              <tr>
        <td colspan="1" align="right" style="padding: 0;">&nbsp;</td>
        <td colspan="10" align="center" style="padding: 0;">FlexiTog<sup>Â®</sup> is a registered brand and trading name of JGM International | VAT Registration no. GB225 3187 77</td>
        <td colspan="1" align="right" style="padding: 0;"><pagenumber/>/<totalpages/></td>
      </tr>
          </table>

    </macro>
    </macrolist>
    <style type="text/css">
      body{margin-top:0px; padding-top:0px;}
      table {
        <#if .locale == "zh_CN">
            font-family: stsong, sans-serif;
        <#elseif .locale == "zh_TW">
            font-family: msung, sans-serif;
        <#elseif .locale == "ja_JP">
            font-family: heiseimin, sans-serif;
        <#elseif .locale == "ko_KR">
            font-family: hygothic, sans-serif;
        <#elseif .locale == "ru_RU">
            font-family: verdana;
        <#else>
            font-family: sans-serif;
        </#if>
            font-size: 8pt;
            table-layout: fixed;
        width:100%
        }


        th {
            font-weight: bold;
            font-size: 8pt;
            vertical-align: top;
            padding: 10px 0px 10px 0px;
            background-color: #333333;
            color: #ffffff;
        }
              
        td {
            padding: 4px 6px;
            color:#868686;
        }

        td#logo{background-color:#cf0a2c; padding: 32px 0px 28px;}
        td#logo img{width:40%; height:40%;}
        /*item table style*/
        table.itemtable > thead > tr > th {
      background-color: #ffffff;
            color: #333333;
            font-style: italic;
            font-size: 10pt;
            vertical-align: bottom;
            font-weight:400;
            padding: 20px 4px 5px 4px;
            border-top:0px;
          }
      table.itemtable > tr {
      padding:0px;
            margin:0px;
          border-bottom:2px solid #ffffff
        }
    table.itemtable > td {
         padding:4px;
         vertical-align:top;
        }
      table.itemtable > tbody > tr > td:first-child, table.itemtable > tbody > tr > td:last-child {background-color:#ffffff; border-bottom:2px solid #ffffff }
      
  table.totals td {
      padding-top:2px;
            padding-bottom:2px;
        }
      
      table.totals > tbody > tr >  td:first-child {
      padding-top:4px;
            padding-bottom:2px;
        }
       table.totals > tbody > tr >  td:last-child {
      padding-top:2px;
            padding-bottom:4px;
        }
      /*inner table*/
      table.inner tr {
          padding:0px;
            margin:0px;
          border-bottom:0px;
      }
      table.inner td {
         padding:0px 4px;
         vertical-align:top;}
      
      
      .underline {
        border-top:2px solid #ffffff!important;
                border-left:0px solid #ffffff!important;
                border-right:0px solid #ffffff!important;
                border-bottom: 2px solid #333333;
                padding-bottom:4px!important;
}
      

      
        /* font styling*/
      
      td.fs4, span.fs4{font-size:4pt}
          td.fs7, span.fs7{font-size:7pt}
      td.fs7-5{font-size:7.5pt}
          td.fs8{font-size:8pt}
      td.fs9{font-size:9pt}
      td.fs10, th.fs10{font-size:10pt}
          td.fs12, tr.fs12{font-size:12pt}
      td.fs14, tr.fs14{font-size:14pt}
      td.fs16, tr.fs16{font-size:16pt}
          td.fs26{font-size:26pt}
          td.fs32{font-size:32pt}
      td.light{font-weight:300;}
      td.normal, th.normal, p.normal, span.normal, .normal{font-weight:400;}
      td.bold, p.bold, span.bold, .bold{font-weight:700;}
      td.heavy, p.heavy, span.heavy, .heavy{font-weight:800;}
      td.italic, th.italic, p.italic, span.italic, .italic{font-style:italic;}
      td.c-black, span.c-dark{color:#1a1a1a;}
          td.c-dark, th.c-dark, span.c-dark{color:#333333;}
      td.c-med, span.c-med{color:#888888;}
      td.c-white, tr.c-white{color:#ffffff;}
          td.c-red, tr.c-red{color:#cf0a2c;}
      td.ar{text-align:right!important;}
            td.tt-lc{text-transform:lowercase;}
            td.tt-uc{text-transform:uppercase;}
          td.tt-sc{text-transform:capitalize;}
            td.ws0{word-spacing:0px;}
      /* Background Colours */
          td.dark, tr.dark{background-color:#333333;}
          td.med, tr.med, th.med, .med{background-color:#ededed;}
          td.med-light, tr.med-light{background-color:#f1f1f1;}
          td.white, tr.white{background-color:#ffffff;}
      /* Padding and margins */
          td.pd-0, tr.pd-0{padding:0pxImportant;}
          td.pdtb-10{padding:10px 0px;}
          td.pdb-10{padding-bottom:10px;}
          td.pdr-2{padding-right:2px}
          .ma-t-10{margin-top:10px;}
          tr.pd-2{padding:2px}
      
      
          
</style>  
</head>
<body header="nlheader" header-height="125" footer="nlfooter" footer-height="20" padding="0mm 0mm 0mm 0mm" size="A4">
<!-- **************************************  DOCUMENT TITLE AND NUMBER **************************************  -->
    <table cellpadding="0">
        <tr>
            <td align="center" colspan="1" >&nbsp;</td>
            <td align="left" colspan="7" class="fs32 c-dark">${record@title}</td>
            <td align="right" colspan="3" class="fs16 c-dark ar "><p align="right">No.${record.tranid}</p></td>
            <td align="center" colspan="1">&nbsp;</td>
        </tr>
    </table>
<!-- **************************************  DOC ADRESSES DATE AND AMOUNTS **************************************  -->
    <table cellpadding="0" style="width: 100%; margin-top:10px; " >
     <!-- Titles -->
  <tr style="padding:5px" >
    <td align="center" colspan="1" >&nbsp;</td>
      <td align="left" colspan="7" class="fs10 c-dark tt-sc">${salesorder.shipaddress@label}</td>
      <td align="right" colspan="3" class="fs10 c-dark ar"  style="padding-right:2px;">${record.trandate@label} <b>${record.trandate}</b></td>
    <td align="center" colspan="1">&nbsp;</td>
  </tr>
      <!-- Details -->
    <tr>
    <td align="center" colspan="1" >&nbsp;</td>
      <td align="left" colspan="7" style="margin-bottom:10px;" class="c-darkc" >${salesorder.shipaddress}</td>
      <td align="right" colspan="3"  class="" >
      <table cellpadding="0" style="padding-bottom:10px; ">
         <tr><td align="right" class="fs26 c-dark bold ar">${record.custbody_nbs121_fulfilboxes}:${record.custbody_nbs121_fulfilboxes@label}</td></tr>
		 <tr><td align="right" class="fs10 c-dark bold ar">Customer Tel: <span style="font-weight: bold;">${record.shipphone}</span></td></tr>
      </table>
    </td>
      <td align="center" colspan="1">&nbsp;</td>
  </tr>
  </table>

<!-- ************************************** ACCOUNT DETAILS ************************************** -->
<table border="0" style="width: 100%;">
  <thead>
    <tr>
      <th align="center" colspan="1" class="pb-0">&nbsp;</th>
      <!-- account details -->
        <th align="left" colspan="3" class="pb-0">
          <p align="left" style="padding-right:1px; line-height:12pt">
          Acc no.&nbsp;<span class="normal">${salesorder.entity.entityid}</span><br/>
          ${salesorder.salesrep@label}&nbsp;<span class="normal">${salesorder.salesrep}</span>
          </p>
        </th>
        <!-- Order ref details -->
        <th align="center" colspan="4" class="pb-0">
          <p align="left" style="padding-right:10px; line-height:12pt">
          <span class="normal">Related records</span><br/>
          ${record.createdfrom}
        </p>
        </th>
        <!-- Shipping and terms details -->
        <th align="center" colspan="3" class="pb-0">
          <p align="right" style="padding-right:1px;  line-height:12pt">
          ${salesorder.custbody_nbs121_ordered_by@label}&nbsp;<span class="normal">${salesorder.custbody_nbs121_ordered_by.entityid}</span><br/>
          ${salesorder.otherrefnum@label}&nbsp;<span class="normal">${salesorder.otherrefnum}</span>
          </p>
        </th>
        <th align="center" colspan="1" class="pb-0">&nbsp;</th>
    </tr>  
  </thead>
</table>
<!-- **************************************  TABLE STARTS HERE **************************************   -->

<#if record.item?has_content>
<table class="itemtable" style="width: 100%; margin-top: 0px;">
<thead style="margin-bottom:2px;">
	<tr>
	<!-- spacer -->
    <th align="center" colspan="1" >&nbsp;</th>
    <!-- Item Code -->
	<th align="left" colspan="2" class="underline">${salesorder.item@label}</th>
	<!-- Item Description -->
	<th align="left" colspan="4" class="underline">${salesorder.item[0].description@label}</th>
	<!-- Pricing -->
	<th align="right" colspan="4" class="underline" style="background-color:#ebebeb;">
		<table border="0" cellpadding="0" style="width: 100%; padding-right:1px;">
			<tr>
				<th align="right" class="med fs10 italic c-dark normal">${salesorder.item[0].quantityordered@label}Ordered</th>
				<th align="right" class="med fs10 italic c-dark normal">Shipped</th>
               <th align="right" class="med fs10 italic c-dark normal"><p align="right">Backorder</p></th>
			</tr>
		</table>
	</th>
	<!-- spacer -->
    <th align="center" colspan="1" >&nbsp;</th>
	</tr>
</thead>
<#if record.showAddlItemsPackSlip == 'NONE'><!-- start items --><#list record.item as item><#list salesorder.item as tranline><#if tranline.line==item.orderline>
	<tr style="background-color: #ececec; border-bottom: 2px solid #ffffff;">
		<!-- spacer -->
		<td align="center" colspan="1" >&nbsp;</td>
		<!-- Item Code -->
		<td align="left" colspan="2" class="fs-9 c-dark" style="border-right:2px solid #fffffff;">${tranline.item}</td>
		<!-- Item Description -->
		<td align="left" colspan="4" class="fs-9 c-dark" style="border-right:2px solid #fffffff;"><span class="itemname">${tranline.description}</span></td>
		<!-- Pricing -->
		<td align="right" colspan="4" style="background-color:#ebebeb;">
			<table border="0" cellpadding="0" class="inner fs-9 c-dark" style="width:100%;" >
				<tr>
					<td align="right" class="fs-9 c-dark">${tranline.quantity}</td>
					<td align="right" class="fs-9 c-dark">${shipped}</td>
                  	<td align="right" class="fs-9 c-dark">${tranline.quantitybackordered}</td>
				</tr>
			</table>
		</td>
		<!-- spacer -->
		<td align="center" colspan="1" >&nbsp;</td>
	</tr>
	</#if></#list></#list><!-- end items --><#elseif record.showAddlItemsPackSlip == 'ALL'><!-- start items from sales order --><#list salesorder.item as tranline><#assign shipped=0><#list record.item as item><#if tranline.line==item.orderline><#assign shipped=item.quantity></#if></#list>
	<tr style="background-color: #f5f5f5; border-bottom: 2px solid #ffffff;">
		<!-- spacer -->
		<td align="center" colspan="1" >&nbsp;</td>
		<!-- Item Code -->
		<td align="left" colspan="2" class="fs-9 c-dark" style="border-right:2px solid #fffffff;">${tranline.item}</td>
		<!-- Item Description -->
		<td align="left" colspan="4" class="fs-9 c-dark" style="border-right:2px solid #fffffff;"><span class="itemname">${tranline.description}</span></td>
		<!-- Pricing -->
		<td align="right" colspan="4" style="background-color:#ebebeb;">
			<table border="0" cellpadding="0" class="inner fs-9 c-dark" style="width:100%;" >
				<tr>
					<td align="right" class="fs-9 c-dark">${tranline.quantity}</td>
					<td align="right" class="fs-9 c-dark">${shipped}</td>
                  	<td align="right" class="fs-9 c-dark">${tranline.quantitybackordered}</td>
				</tr>
			</table>
		</td>
		<!-- spacer -->
		<td align="center" colspan="1" >&nbsp;</td>
	</tr>
	</#list><!-- end sales order items --><#elseif record.showAddlItemsPackSlip == 'UNFULFILLED'><!-- start items from sales order --><#list salesorder.item as tranline><#assign shipped=0><#assign isInList=false><#list record.item as item><#if tranline.line==item.orderline><#assign shipped=item.quantity><#assign isInList=true></#if></#list><#if isInList || tranline.quantitybackordered != 0>
	<tr style="background-color: #c6c6c6; border-bottom: 2px solid #ffffff;">
		<!-- spacer -->
		<td align="center" colspan="1" >&nbsp;</td>
		<!-- Item Code -->
		<td align="left" colspan="2" class="fs-9 c-dark" style="border-right:2px solid #fffffff;">${tranline.item}</td>
		<!-- Item Description -->
		<td align="left" colspan="4" class="fs-9 c-dark" style="border-right:2px solid #fffffff;"><span class="itemname">${tranline.description}</span></td>
		<!-- Pricing -->
		<td align="right" colspan="4" style="background-color:#ebebeb;">
			<table border="0" cellpadding="0" class="inner fs-9 c-dark" style="width:100%;" >
				<tr>
					<td align="right" class="fs-9 c-dark">${tranline.quantity}</td>
					<td align="right" class="fs-9 c-dark">${shipped}</td>
                  	<td align="right" class="fs-9 c-dark">${tranline.quantitybackordered}</td>
				</tr>
			</table>
		</td>
		<!-- spacer -->
		<td align="center" colspan="1" >&nbsp;</td>
	</tr>
	</#if></#list><!-- end sales order items --></#if>
	
</table>
</#if>


<!--  **************************************  Customer Delivery Notes **************************************  -->
    <table style="page-break-inside: avoid; width: 100%; margin-top:20px; " >
	<tr>
		<td align="center" colspan="1" class="white" >&nbsp;</td>
		<td align="left" colspan="10" class="fs10 c-dark" style="border-top:1px dotted #333333; padding-top:6px;">${salesorder.custbody_nbs121_customer_note@label}</td>
		<td align="center" colspan="1" class="white" >&nbsp;</td> 
	</tr>
	<tr>
		<td align="center" colspan="1" class="white" >&nbsp;</td>
		<td align="Center" colspan="10" class="c-dark">${salesorder.custbody_nbs121_customer_note}</td>
		<td align="center" colspan="1" class="white" >&nbsp;</td> 
    </tr>
	<tr class="pd-0" style="margin:0px; padding:0px">
		<td align="center" colspan="1" class="white pd-0 fs4" >&nbsp;</td>
		<td colspan="10" class="pd-0 fs4" style="border-bottom:1px dotted #333333;">&nbsp;</td>
		<td align="center" colspan="1" class="white pd-0 fs4" >&nbsp;</td>
	</tr>
<!--  **************************************  Discalimer **************************************  -->
</table>
<table style="page-break-inside: avoid; width: 100%; margin-top:20px; " >
	<tr>
		<td align="center" colspan="1" class="white" >&nbsp;</td>
		<td align="center" colspan="10" class="fs14 c-red" >
			<p align="center">Please ensure all boxes are accounted for before signing for your delivery<br/>
				<span style="font-size:8pt; color:#1a1a1a;">FlexiTog will not take responsibility for any undelivered boxes if they have been signed for.</span>
			</p>
		</td>
		<td align="center" colspan="1" class="white" >&nbsp;</td> 
	</tr>
</table>
</body>
</pdf>