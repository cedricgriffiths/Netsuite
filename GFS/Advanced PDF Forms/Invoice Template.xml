    <style type="text/css">table {
            <!--font-family: sans-serif;-->
      		font-family: Calibri, Candara, Segoe, "Segoe UI", Optima, Arial, sans-serif;
            font-size: 9pt;
            table-layout: fixed;
        }
        th {
            font-weight: bold;
            font-size: 8pt;
            padding: 0px;
            <!--background-color: #e3e3e3;-->
            border-bottom: 1px solid black;
    		border-collapse: collapse;
            <!--color: #333333;-->
        }
        td {
            padding: 0px;
            vertical-align: top;
            font-size:10px;
        }
        b {
            font-weight: bold;
            color: #333333;
        }
        table.header td {
            padding: 0px;
            font-size: 10pt;
        }
        table.footer td {
            padding: 0;
            font-size: 6pt;
        }
        table.itemtable th {
            padding-bottom: 0px;
            padding-top: 0px;

        }
        table.body td {
            padding-top: 0px;
        }
        table.total {
            page-break-inside: avoid;
        }
      table.message{
        border: 1px solid #dddddd;
      }
        tr.totalrow {
            <!--background-color: #e3e3e3;-->
            line-height: 200%;
        }
      tr.messagerow{
        font-size: 6pt;
      }
        td.totalboxtop {
            font-size: 12pt;
            background-color: #e3e3e3;
        }
        td.addressheader {
            font-size: 10pt;
            padding-top: 0px;
            padding-bottom: 0px;
        }
        td.address {
            padding-top: 0;
          font-size: 10pt;
        }
        td.totalboxmid {
            font-size: 28pt;
            padding-top: 20px;
            background-color: #e3e3e3;
        }
        td.totalcell {
          border-bottom: 1px solid black;
    		border-collapse: collapse;
        }
      td.message{
        font-size: 8pt;
      }
