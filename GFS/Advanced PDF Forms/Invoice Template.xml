    <macrolist>
        <macro id="nlheader">
            <table class="header" style="width: 100%;">
    <tr>
	<td align="right">&nbsp;</td>
	<td align="right">&nbsp;</td>
	<td align="right"><img src="${logoInfo}" style="${logoStyle}" /></td>
    </tr>
    <!--<tr><td>&nbsp;</td></tr>-->
    <tr>
	<td style="padding-top: 10px;"><span style="font-size:24px;">${record@title}</span></td>
	<td align="right">&nbsp;</td>
	<td align="right">&nbsp;</td>
	</tr>
          </table>
    <table class="header" style="width: 100%;">

	<tr>
	<td align="right">&nbsp;</td>
	<td align="right">&nbsp;</td>
	<td align="right">&nbsp;</td>
	<td align="right">&nbsp;</td>
	</tr>
	<tr>
        <td colspan="2" rowspan="8" class="addressheader"><span style="font-size:10pt"><b>Invoice Address:</b></span><br /><span class="nameandaddress" style="font-size:10pt">${record.billaddress}<br/>VAT Number: ${record.vatregnum}</span></td>
	<td align="right" style="font-size:10pt"></td>
      
    <td colspan="2" align="left" rowspan="8"><span class="nameandaddress">${record.custbody_subsidiary_address}</span><br/>VAT No. ${vatInfo}<br /><br/><b>Tel.</b> ${telInfo}</td>
	</tr>
          </table>
        </macro>

        <macro id="nlfooter">

        <table style="width: 100%;">
          <#if record.duedate?has_content>
          	<tr><td><b>Due Date: ${record.duedate?string["dd MMM yyyy"]}</b></td></tr>
          <#else>
            <tr><td><b>Due Date:</b></td></tr>
          </#if>
          <tr><td>Bank Details for payments in ${record.currency}:</td></tr>
          <tr><td>${record.custbody_bbs_bank_account}</td><td align="right" rowspan="7">&nbsp;</td></tr>
          <tr><td>${record.custbody_bbs_bank_name}</td></tr>
          <tr><td>Account Number: ${record.custbody_bbs_account_no}</td></tr>
          <tr><td>Sort Code: ${record.custbody_bbs_sort_code}</td></tr>
          <tr><td>IBAN: ${record.custbody_bbs_iban}</td></tr>
          <tr><td>Swift Code: ${record.custbody_bbs_bic}</td></tr>
          <tr><td>&nbsp;</td></tr>
        </table>

       <table style="width: 100%;">
         
          <tr class="messagerow">
              <td align="left" class="message" style="margin-top: 10px;">${regInfo}</td>
		  </tr>
        </table>

		<table class="footer" style="width: 100%;">
          <tr>
			<td align="right"><pagenumber/> of <totalpages/></td>
		</tr>
        </table>

