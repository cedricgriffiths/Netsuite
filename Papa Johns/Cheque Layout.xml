<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>
<#if .locale == "ru_RU">
    <link name="verdana" type="font" subtype="opentype" src="${nsfont.verdana}" src-bold="${nsfont.verdana_bold}" bytes="2" />
</#if>
    <style type="text/css">.check table {
            position: relative;
            overflow: hidden;
            font-size: 10pt;
            font-family: Helvetica, sans-serif;
        }
</style>
</head>
<body padding="0cm 1.2cm 0cm 1.2cm" size="A4">
    <#list records as check>
<div style="position: relative;font-family: Helvetica,sans-serif;top: 0pt;height: 280pt;width: 612pt;page-break-inside: avoid;font-size: 8pt;">


<table style="position: absolute;overflow: hidden;left: -8pt;top: 82pt;height: 18pt;width: 108pt;"><tr>
	<td>${check.trandate}</td>
	</tr></table>

<table style="position: absolute;overflow: hidden;left: 60pt;top: 82pt;height: 18pt;width: 393pt;"><tr>
	<td>${check.entity.altname}</td>
	</tr></table>

<#if (check.usertotal?length > 0)>
  <#assign totalValue>${check.usertotal}</#assign>
<#else>
  <#assign totalValue>${check.total}</#assign>
</#if>

<#assign dotPos>${totalValue?index_of(".")}</#assign>
<#assign dotPos2 = 1>
<#assign dotPos3 = dotPos2?number + dotPos?number>
<#assign pence = totalValue[dotPos3..]>
  
  
<table style="position: absolute;overflow: hidden;left: 434pt;top: 100pt;height: 18pt;width: 111pt;"><tr>
	<td>${totalValue?replace("Â£","")}</td>
	</tr></table>
  
<table style="position: absolute;overflow: hidden;left: 50pt;top: 120pt;height: 18pt;width: 30pt;">
<tr>
	<td>${check.wordHundredThousands?upper_case}</td>
</tr>
</table>
  
<table style="position: absolute;overflow: hidden;left: 102pt;top: 120pt;height: 18pt;width: 30pt;">
<tr>
	<td>${check.wordTenThousands?upper_case}</td>
</tr>
</table>
  
<table style="position: absolute;overflow: hidden;left: 150pt;top: 120pt;height: 18pt;width: 30pt;">
<tr>
	<td>${check.wordThousands?upper_case}</td>
</tr>
</table>
  
<table style="position: absolute;overflow: hidden;left: 197pt;top: 120pt;height: 18pt;width: 30pt;">
<tr>
	<td>${check.wordHundreds?upper_case}</td>
</tr>
</table>
  
  <table style="position: absolute;overflow: hidden;left: 247pt;top: 120pt;height: 18pt;width: 30pt;">
<tr>
	<td>${check.wordTens?upper_case}</td>
</tr>
</table>
  
  <table style="position: absolute;overflow: hidden;left: 292pt;top: 120pt;height: 18pt;width: 30pt;">
<tr>
	<td>${check.wordOnes?upper_case}</td>
</tr>
</table>

<table style="position: absolute;overflow: hidden;left: 357pt;top: 120pt;height: 18pt;width: 30pt;">
<tr>
	<td>${pence}</td>
</tr>
</table>

</div>
</#list>
</body>
</pdf>