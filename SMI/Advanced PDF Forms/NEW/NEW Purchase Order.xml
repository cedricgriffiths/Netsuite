    </macrolist>    <style type="text/css">body{margin-top:0px; padding-top:0px;}
      table {
        <#if .locale == "zh_CN">
            font-family: stsong, sans-serif;
        <#elseif .locale == "zh_TW">
            font-family: msung, sans-serif;
        <#elseif .locale == "ja_JP">
            font-family: heiseimin, sans-serif;
        <#elseif .locale == "ko_KR">
            font-family: hygothic, sans-serif;
        <#elseif .locale == "ru_RU">
            font-family: verdana;
        <#else>
            font-family: sans-serif;
        </#if>
            font-size: 8pt;
            table-layout: fixed;
        width:100%
        }


        th {
            font-weight: bold;
            font-size: 8pt;
            vertical-align: top;
            padding: 10px 0px 10px 0px;
            background-color: #333333;
            color: #ffffff;
        }
              
        td {
            padding: 4px 6px;
            color:#868686;
        }

        td#logo img{width:120%; height:120%;}

        /*item table style*/
        table.itemtable > thead > tr > th {
      background-color: #ffffff;
            color: #333333;
            font-style: italic;
            font-size: 10pt;
            vertical-align: bottom;
            font-weight:400;
            padding: 20px 4px 5px 4px;
            border-top:0px;
          }
      table.itemtable > tr {
      padding:0px;
            margin:0px;
          border-bottom:2px solid #ffffff
        }
    table.itemtable > td {
         padding:4px;
         vertical-align:top;
        }
      table.itemtable > tbody > tr > td:first-child, table.itemtable > tbody > tr > td:last-child {background-color:#ffffff; border-bottom:2px solid #ffffff }
      
  table.totals td {
      padding-top:2px;
            padding-bottom:2px;
        }
      
      table.totals > tbody > tr >  td:first-child {
      padding-top:4px;
            padding-bottom:2px;
        }
       table.totals > tbody > tr >  td:last-child {
      padding-top:2px;
            padding-bottom:4px;
        }
      /*inner table*/
      table.inner tr {
          padding:0px;
            margin:0px;
          border-bottom:0px;
      }
      table.inner td {
         padding:0px 4px;
         vertical-align:top;}
      
      
      .underline {
        border-top:2px solid #ffffff!important;
                border-left:0px solid #ffffff!important;
                border-right:0px solid #ffffff!important;
                border-bottom: 2px solid #333333;
                padding-bottom:4px!important;
}
      

      
        /* font styling*/
      
      td.fs4, span.fs4{font-size:4pt}
      td.fs7, span.fs7{font-size:7pt}
      td.fs7-5{font-size:7.5pt}
      td.fs8{font-size:8pt}
      td.fs9{font-size:9pt}
      td.fs10, th.fs10{font-size:10pt}
      td.fs12, tr.fs12{font-size:12pt}
      td.fs16, tr.fs16{font-size:16pt}
      td.fs26{font-size:26pt}
      td.fs32{font-size:32pt}
      td.light{font-weight:300;}
      td.normal, th.normal, p.normal, span.normal, .normal{font-weight:400;}
      td.bold, p.bold, span.bold, .bold{font-weight:700;}
      td.heavy, p.heavy, span.heavy, .heavy{font-weight:800;}
      td.italic, th.italic, p.italic, span.italic, .italic{font-style:italic;}
      td.c-black, span.c-dark{color:#1a1a1a;}
      td.c-dark, th.c-dark, span.c-dark{color:#333333;}
      td.c-med, span.c-med{color:#888888;}
      td.c-white, tr.c-white{color:#ffffff;}
      td.c-red, tr.c-red{color:#cf0a2c;}
      td.ar{text-align:right!important;}
      td.tt-lc{text-transform:lowercase;}
      td.tt-uc{text-transform:uppercase;}
      td.tt-sc{text-transform:capitalize;}
      td.ws0{word-spacing:0px;}
      /* Background Colours */
      td.dark, tr.dark{background-color:#333333;}
      td.med, tr.med, th.med, .med{background-color:#ededed;}
      td.med-light, tr.med-light{background-color:#f1f1f1;}
      td.white, tr.white{background-color:#ffffff;}
      /* Padding and margins */
      td.pd-0, tr.pd-0{padding:0pxImportant;}
      td.pdtb-10{padding:10px 0px;}
      td.pdb-10{padding-bottom:10px;}
      td.pdr-2{padding-right:2px}
         .ma-t-10{margin-top:10px;}
      tr.pd-2{padding:2px}
</style>
</head>
<body header="nlheader" header-height="125" footer="nlfooter" footer-height="20" padding="0mm 0mm 0mm 0mm" size="A4">
    <!-- **************************************  DOCUMENT TITLE AND NUMBER **************************************  -->
<table cellpadding="0"><tr>
	<td align="center" colspan="1">&nbsp;</td>
	<td align="left" class="fs32 c-dark" colspan="7">${record@title}</td>
	<td align="right" class="fs16 c-dark ar " colspan="3">
	<p align="right">No.${record.tranid}</p>
	</td>
	<td align="center" colspan="1">&nbsp;</td>
	</tr></table>
<!-- **************************************  DOC ADRESSES DATE AND AMOUNTS **************************************  -->

<table cellpadding="0" style="width: 100%; margin-top:10px;"><!-- Titles --><tr style="padding:5px">
	<td align="center" colspan="1">&nbsp;</td>
	<td align="left" class="fs10 c-grey tt-sc" colspan="4" style="padding-right:15px;">
	<p align="left">${record.billaddress@label}</p>
	</td>
	<td align="left" class="fs10 c-grey tt-sc" colspan="4">
	<p align="left">Delivery Address</p>
	</td>
	<td align="right" class="fs10 c-dark ar" colspan="2" style="padding-right:2px;">Order Date <b>${record.trandate}</b></td>
	<td align="center" colspan="1">&nbsp;</td>
	</tr>
	<!-- Details -->
	<tr style="padding-bottom:15px;">
	<td align="center" colspan="1">&nbsp;</td>
	<td align="left" colspan="4" style="padding-right:8px;">
	<p align="left">${record.billaddress}</p>
	</td>
	<td align="left" colspan="4">
	<p align="left">${record.shipaddress}</p>
	</td>
	<td align="right" colspan="2">&nbsp;</td>
	<td align="center" colspan="1">&nbsp;</td>
	</tr></table>
<!-- ************************************** ACCOUNT DETAILS ************************************** -->

<table border="0" style="width: 100%;">
<thead>
	<tr>
	<th align="center" class="pb-0" colspan="1">&nbsp;</th>
	<!-- account details -->
	<th align="left" class="pb-0" colspan="5">
	<p align="left" style="padding-right:1px; line-height:12pt">Account No.&nbsp;<span class="normal">${record.entity.custentity_bbs_legacy_accnumber}</span> <!--<br />${record.otherrefnum@label}&nbsp;<span class="normal">${record.otherrefnum}</span>--></p>
	</th>
	<!-- Order ref details -->
	<th align="right" class="pb-0" colspan="5">
	<p align="right" style="padding-right:10px; line-height:12pt">Delivery Date&nbsp;<span class="normal">${record.duedate?string["dd MMM yyyy"]}</span><!-- <br />${record.tranid@label}&nbsp;<span class="normal">${record.tranid}</span> --></p>
	</th>
	<th align="center" class="pb-0" colspan="1">&nbsp;</th>
	</tr>
</thead>
</table>
<!-- **************************************  TABLE STARTS HERE **************************************   --><#if record.item?has_content>

<table class="itemtable" style="width: 100%; margin-top: 0px;"><!-- Start Items --><#list record.item as item><#if item_index==0><!-- ******************************* TABLE HEADINGS ********************************   -->
<thead style="margin-bottom:2px;">
	<tr><!-- spacer -->
	<th align="center" colspan="1">&nbsp;</th>
	<!-- Item Code -->
	<th align="left" class="underline" colspan="2">Supplier Code</th>
    <!-- Item Description -->
	<th align="left" class="underline" colspan="5">Description</th>
	<!-- Pricing -->
	<th align="center" class="underline" colspan="3" style="background-color:#ebebeb;">
	<table border="0" cellpadding="0" style="width: 100%; padding-right:1px;"><tr>
		<th align="center" class="med fs10 italic c-dark normal">${item.quantity@label}</th>
		<th align="right" class="med fs10 italic c-dark normal">${item.rate@label}</th>
		<th align="right" class="med fs10 italic c-dark normal">Amount</th>
		</tr></table>
	</th>
	<!-- spacer -->
	<th align="center" colspan="1">&nbsp;</th>
	</tr>
</thead>
</#if><!-- ******************************* TABLE COLUMNS ********************************   --><tr style="background-color: ${((item_index % 2)==0)?string('#f6f6f6', '#ffffff')};  border-bottom: 2px solid #ffffff; "><!-- spacer -->
	<td align="center" colspan="1">&nbsp;</td>
	<!-- Item Code -->
	<td align="left" class="fs-9 c-dark" colspan="2" style="border-right:2px solid #fffffff;">${item.vendorname}</td>
  <!-- Item Description -->
	<td align="left" class="fs-9 c-dark" colspan="5" style="border-right:2px solid #fffffff;">${item.item} <!--${item.description} --></td>
	<!-- Pricing -->
	<td align="center" colspan="3" style="background-color: ${((item_index % 2)==0)?string('#f6f6f6', '#ebebeb')}; ">
	<table border="0" cellpadding="0" class="inner fs-9 c-dark" style="width: 100%;"><tr>
		<td align="center" class="fs-9 c-dark">${item.quantity}</td>
		<td align="right" class="fs-9 c-dark">${item.rate}</td>
		<td align="right" class="fs-9 c-dark">${item.grossamt}</td>
		</tr></table>
	</td>
	<!-- spacer -->
	<td align="center" colspan="1">&nbsp;</td>
	</tr>
	</#list></table>
<!-- end items --></#if><!--  **************************************  TABLE TOTALS COLUMN STARTS HERE **************************************  -->

<table class="totals" style="page-break-inside: avoid; width: 100%; margin-top:0px;"><!-- ************ Sub total ************ --><tr class="med" style=""><!-- spacer -->
	<td align="center" class="white" colspan="1">&nbsp;</td>
	<td align="center" class="white fs12 black italic" colspan="7" style="padding:0px;" valign="middle">&nbsp;</td>
	<!-- CONTENT GOES HERE -->
	<td align="left" class="bold c-black" colspan="2" style="padding-top:10px;">${record.subtotal@label}</td>
	<td align="right" class="normal c-black" colspan="1" style="padding-top:10px;">${record.subtotal}</td>
	<!-- spacer -->
	<td align="center" class="white" colspan="1">&nbsp;</td>
	</tr>
	<!--  ************ Shipping ************  -->
	<tr class="med"><!-- spacer -->
	<td align="center" class="white" colspan="1">&nbsp;</td>
	<td align="center" class="white fs12 black italic" colspan="7" style="padding:0px;" valign="middle">&nbsp;</td>
	<td align="left" class="bold c-black" colspan="2">${record.shippingcost@label}</td>
	<td align="right" class="normal c-black " colspan="1">${record.shippingcost}</td>
	<!-- spacer -->
	<td align="center" class="white" colspan="1">&nbsp;</td>
	</tr>
	<!-- ************ VAT ************  -->
	<tr class="med"><!-- spacer -->
	<td align="center" class="white" colspan="1">&nbsp;</td>
	<td align="center" class="white fs12 black italic" colspan="7" style="padding:0px;" valign="middle">&nbsp;</td>
	<td align="left" class="bold c-black " colspan="2">VAT</td>
	<td align="right" class="normal c-black " colspan="1">${record.taxtotal}</td>
	<!-- spacer -->
	<td align="center" class="white" colspan="1">&nbsp;</td>
	</tr>
	<!-- ************ Totals ************  -->
	<tr class="dark bold fs10 c-white;">
	<td align="center" class="white" colspan="1">&nbsp;</td>
	<td align="center" class="c-dark fs9 white" colspan="7" style="" valign="middle">&nbsp;</td>
	<td colspan="3"><!--  ************************************** Totals and terms **************************************  -->
	<table cellpadding="0" style="padding: 4px 0px;"><tr>
		<td align="left" class="c-white fs16" valign="middle">${record.total@label}</td>
		<td align="right" class="c-white fs12" valign="middle">${record.total}</td>
		</tr></table>
	</td>
	<td align="center" class="white" colspan="1">&nbsp;</td>
	</tr></table>

</body>
</pdf>