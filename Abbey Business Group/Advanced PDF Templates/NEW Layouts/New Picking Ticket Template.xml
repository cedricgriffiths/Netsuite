<?xml version="1.0"?><!DOCTYPE pdf PUBLIC "-//big.faceless.org//report" "report-1.1.dtd">
<pdf>
<head>

    <macrolist>
        <macro id="nlheader">
            <table class="header" style="width: 100%;"><tr>
	<td>&nbsp;</td>
	<td align="right">&nbsp;</td>
	<td align="right">&nbsp;</td>
	<td align="right" rowspan="8"><#if companyInformation.logoUrl?length != 0><img src="${companyInformation.logoUrl}" style="float: right; width:295px; height:180px;" /> </#if></td>
	</tr>
	<tr><td><span style="font-size:18px; color:red; font-weight: bold;">PICKING TICKET</span></td><td align="right">&nbsp;</td></tr>
	<tr><td rowspan="5"><span style="font-size:10px;">${companyInformation.addressText}</span></td><td align="right">&nbsp;</td><td align="right">&nbsp;</td></tr>
	<tr><td align="left">&nbsp;</td></tr>
	<tr><td align="left">&nbsp;</td></tr>
	<tr><td align="left">&nbsp;</td></tr>
	<tr><td align="left">&nbsp;</td></tr>
	<tr><td align="left" colspan="2" style="font-size:10px; font-weight: bold;">Tel: 01242 263444</td></tr>
	<tr><td align="left" colspan="2" style="width: 100%; font-size:10px; font-weight: bold;">Email: accounts@abbeybusinessgroup.com</td></tr>
	<tr><td align="left" colspan="2" style="width: 100%; font-size:10px; font-weight: bold;">Web: www.abbeybusinessgroup.com</td></tr>
    <tr><td align="left">&nbsp;</td></tr>
	</table>

    <table style="width: 100%;">
	<tr>
	<td align="left" colspan="2" style="font-size:10px; background-color:#5b5c61; color:#ffffff; border-bottom: 2px solid white; padding: 5px;">Deliver To</td>
	<td align="left">&nbsp;</td>
	<td align="left" style="font-size:10px; background-color:#5b5c61; color:#ffffff; border-bottom: 2px solid white; border-right: 2px solid white; padding: 5px;">ORDER NO:</td>
	<td align="left" style="font-size:10px; background-color:#d6d6d7; color:#000000; border-bottom: 2px solid white; padding: 5px;">${record.tranid}</td>
	</tr>

	<tr>
	<td align="left" colspan="2" rowspan="5" style="font-size:10px; background-color:#d6d6d7; color:#000000; padding: 5px;">${record.shipaddress}</td>
    <td align="left">&nbsp;</td>
	<td align="left" rowspan="1" style="font-size:10px; background-color:#5b5c61; color:#ffffff; border-bottom: 2px solid white; border-right: 2px solid white; padding: 5px;">Order Date:</td>
	<td align="left" rowspan="1" style="font-size:10px; background-color:#d6d6d7; color:#000000; border-bottom: 2px solid white; padding: 5px;">${record.trandate}</td>
	</tr>


    <tr>
	<td align="left">&nbsp;</td>
	<td align="left"  style="font-size:10px; background-color:#5b5c61; color:#ffffff; border-bottom: 2px solid white; border-right: 2px solid white; padding: 5px;">Project No:</td>
	<td align="left"  style="font-size:10px; background-color:#d6d6d7; color:#000000; border-bottom: 2px solid white; padding: 5px;">${record.custbody_cseg_bbs_project_no}</td>
	</tr>
    <tr>

	<td align="left">&nbsp;</td>
	<td align="left"  style="font-size:10px; background-color:#5b5c61; color:#ffffff; border-bottom: 2px solid white; border-right: 2px solid white; padding: 5px;">Project Title:</td>
	<td align="left"  style="font-size:10px; background-color:#d6d6d7; color:#000000; border-bottom: 2px solid white; padding: 5px;">${record.custbody_bbs_so_project_text}</td>
	</tr>

	</table>
</macro>
    <macro id="nlfooter">

<table class="footer" style="width: 100%;">
  	<tr>
    <td align="center" style="font-size: 6pt">REG. OFFICE NO. 2200765 REG. OFFICE: Units 23 &amp; 24 Neptune Business Centre, Tewkesbury Road, Cheltenham, Gloucestershire. GL51 9FB VAT REG. No. 484 6151 31</td>
	</tr>
  	<tr>
    <td align="right"><pagenumber/> of <totalpages/></td>
	</tr>
</table>
        </macro>
    </macrolist>
  
    <style type="text/css">table {
            font-family: helvetica;
            font-size: 9pt;
            table-layout: fixed;
        }
        th {
            font-weight: bold;
            font-size: 8pt;
            padding: 0px;
            background-color: #e3e3e3;
            color: #333333;
        }
        td {
            padding: 0px;
            vertical-align: middle;
            font-size:12px;
        }
        b {
            font-weight: bold;
            color: #333333;
        }
        table.header td {
            padding: 0px;
            font-size: 10pt;
        }
        table.footer td {
            padding: 0;
            font-size: 8pt;
        }
        table.itemtable th {
            padding-bottom: 0px;
            padding-top: 0px;
        }
        table.itemtable td {
          font-size: 9pt;
        }
        table.body td {
            padding-top: 0px;
        }
        table.total {
            page-break-inside: avoid;
        }
      table.message{
        border: 1px solid #dddddd;
      }
        tr.totalrow {
            background-color: #e3e3e3;
            line-height: 125%;
        }
      tr.messagerow{
        font-size: 6pt;
      }
        td.totalboxtop {
            font-size: 12pt;
            background-color: #e3e3e3;
        }
        td.addressheader {
            font-size: 12pt;
            padding-top: 0px;
            padding-bottom: 0px;
        }
        td.address {
            padding-top: 0;
        }
        td.totalboxmid {
            font-size: 28pt;
            padding-top: 20px;
            background-color: #e3e3e3;
        }
      td.message{
        font-size: 6pt;
        font-weight: bold;
      }
        td.totalboxbot {
            background-color: #e3e3e3;
            font-weight: bold;
        }
      td.itemtable {
      white-space: pre;
      }
        span.title {
            font-size: 28pt;
        }
        span.number {
            font-size: 16pt;
        }
        span.itemname {
            font-weight: bold;
            line-height: 150%;
        }
        hr {
            width: 100%;
            color: #d3d3d3;
            background-color: #d3d3d3;
            height: 1px;
        }
</style>
</head>
<body header="nlheader" header-height="350px" footer="nlfooter" footer-height="40px" padding="0.2cm 0.5cm 0.2cm 0.5cm" size="A4">

  <#if record.item?has_content>

<table class="itemtable" style="width: 100%; margin-top: 10px;"><!-- start items --><#list record.item as item><#if item_index==0>
<thead>
	<tr>
	<th align="left" colspan="19" style="font-size:px; background-color:#5b5c61; color:#ffffff; border-right: 2px solid white; padding: 5px;">${item.item@label}</th>
	<th align="right" colspan="4" style="font-size:px; background-color:#5b5c61; color:#ffffff; border-right: 2px solid white; padding: 5px;">${item.quantity@label}</th>
	<th align="right" colspan="4" style="font-size:px; background-color:#5b5c61; color:#ffffff; border-right: 2px solid white; padding: 5px;">${item.units@label}</th>
	<th align="right" colspan="4" style="font-size:px; background-color:#5b5c61; color:#ffffff; padding: 5px;">${item.quantitycommitted@label}</th>
	</tr>
</thead>
</#if>
  <tr>
	<td align="left" line-height="150%" colspan="19" style="font-size:10px; background-color:#d6d6d7; color:#000000; border-right: 2px solid white; padding: 5px;">${item.item}<br/>${item.description}</td>
	<td align="right" colspan="4" style="font-size:10px; background-color:#d6d6d7; color:#000000; border-right: 2px solid white; padding: 5px;">${item.quantity}</td>
	<td align="right" colspan="4" style="font-size:10px; background-color:#d6d6d7; color:#000000; border-right: 2px solid white; padding: 5px;">${item.units}</td>
	<td align="right" colspan="4" style="font-size:10px; background-color:#d6d6d7; color:#000000; padding: 5px;">${item.quantitycommitted}</td>
	</tr>
	</#list><!-- end items --></table>
</#if>

<hr />

</body>
</pdf>